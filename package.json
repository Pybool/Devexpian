{
  "name": "@expian/cypress",
  "private": true,
  "version": "2.1.4",
  "description": "Expian E2E Cypress testing suit",
  "main": "index.js",
  "author": "expian",
  "license": "ISC",
  "scripts": {
    "cy:open": "cypress open",
    "lint": "eslint .\\**\\*.{js,tsx,jsx,ts}",
    "lint:fix": "prettier --write . && eslint './**/*.{js,tsx,jsx,ts}' --fix",
    "lint:debug": "DEBUG=eslint:cli-engine eslint --quiet './**/*.{js,tsx,jsx,ts}'",
    "lint:report": "eslint --quiet --output-file eslint_report.json --format json './**/*.{js,tsx,jsx,ts}'",
    "start:regression-RS2": "cypress run -e TAGS='test' --headless --env testenv=booking",
    "generate:report": "node cucumber-html-report.js",
    "initialize:testrail": "node testrail.js --init",
    "update:testrail-sections": "node testrail.js --createsubdirs",
    "export:reports:bookings": "node testrail.js --export --Bookings",
    "export:reports:reservations": "node testrail.js --export --Reservations"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/integration",
    "filterSpecs": true,
    "omitFiltered": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "./cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "axios": "^1.4.0",
    "axios-rate-limit": "^1.3.0",
    "btoa": "^1.2.1",
    "chance": "^1.1.11",
    "cypress-image-diff-js": "^1.27.0",
    "json-stable-stringify": "^1.0.2",
    "lodash": "^4.17.21",
    "luxon": "3.4.0",
    "nanoid": "^4.0.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.1",
    "cypress": "12.17.4",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-dotenv": "^2.0.0",
    "cypress-iframe": "^1.0.1",
    "cypress-vite": "^1.4.2",
    "dotenv": "^16.3.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-ticknovate": "^1.0.9",
    "jest-environment-jsdom": "^29.6.2",
    "multiple-cucumber-html-reporter": "^3.4.0",
    "prettier": "^3.0.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  }
}
